<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="11000" id="com.golftourney.golftourney" version="1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GolfTourney</name>
    <description>
        GolfTourney.com mobile App.
    </description>
    <author email="bbaago@gmail.com" href="golftourney.com">
        GolfTourney.com
    </author>
    <content src="login.html" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="StatusBarBackgroundColor" value="#32AB34" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="orientation" value="portrait" />
</widget>
